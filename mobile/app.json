{
  "expo": {
    "name": "GPA Gine",
    "slug": "gpa-gine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gpa-gine",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.crispengari.gpagine"
    },
    "android": {
      "package": "com.crispengari.gpagine",
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/notifications.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": ["./assets/sounds/notifications.wav"],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-quick-actions",
        {
          "androidIcons": {
            "performance": {
              "foregroundImage": "./assets/icons/performance-android.png",
              "backgroundColor": "#ffffff"
            },
            "history": {
              "foregroundImage": "./assets/icons/history-android.png",
              "backgroundColor": "#ffffff"
            },
            "settings": {
              "foregroundImage": "./assets/icons/settings-android.png",
              "backgroundColor": "#ffffff"
            }
          },
          "iosIcons": {
            "performance": {
              "1x": "./assets/icons/performance.png"
            },
            "history": {
              "1x": "./assets/icons/history.png"
            },
            "settings": {
              "1x": "./assets/icons/settings.png"
            }
          },
          "iosActions": [
            {
              "id": "performance",
              "title": "Performance",
              "subtitle": "Check your GPA performance",
              "icon": "performance",
              "params": {
                "href": "/(questions)/(health)/anxiety"
              }
            },
            {
              "id": "history",
              "title": "History",
              "subtitle": "Check your GPA history",
              "icon": "history",
              "params": {
                "href": "/(tabs)/history"
              }
            },
            {
              "id": "settings",
              "title": "Settings",
              "subtitle": "Open your Settings",
              "icon": "settings",
              "params": {
                "href": "/(tabs)/settings"
              }
            }
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b54c6b4d-feff-4f81-8657-d8cb07707d63"
      }
    }
  }
}
